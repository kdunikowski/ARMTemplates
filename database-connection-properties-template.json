{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "accesKeyProperties": {
            "type":"array"
        }
    },
    "variables": {
    },
    "resources": 
    [],
    "outputs" : {
        "properties":{
            "type":"array",
             "copy": {
                "count": "[length(parameters('accesKeyProperties'))]",
                "input":
                {
                    "name": "[parameters('accesKeyProperties')[copyIndex()].name]",
                    "value": "[listConnectionStrings(resourceId(parameters('accesKeyProperties')[copyIndex()].resourceType, parameters('accesKeyProperties')[copyIndex()].resourceName),  parameters('accesKeyProperties')[copyIndex()].apiVersion).connectionStrings[0].connectionString]"
                }
            }
        }
    }
}